
@import url("http://fonts.googleapis.com/css?family=Roboto+Condensed:700,400|Roboto:500,900,300,700,400&subset=latin,greek-ext");

.html 
{
	overflow: hidden;
}

body 
{
	margin: 15px;
	background: url(/images/body-bkg.png) repeat-y center top;
	background-color: #C6E5F2;
}

p, span 
{
	margin: 0px;
	padding: 0px;
	line-height: 1.5;
}

hr 
{
	background: #DBE5FF;
	border: 1px solid #DBE5FF;
	height: 2px;
}

h4 
{
	color: #35F;
	font-weight: bold;
	font-size: 22px;
	font-weight: 700;
	font-family: Roboto, Cambria, Calibri, Sans-Serif;
	text-decoration: none;
	margin-top: 0px;
	margin-bottom: 5px;
	padding-top: 0px;
	padding-bottom: 10px;
}

h5 
{
	color: #46F;
	font-weight: bold;
	font-size: 14px;
	font-weight: 700;
	font-family: Roboto, Cambria, Calibri, Sans-Serif;
	text-decoration: none;
	margin-bottom: 4px;
	margin-top: 4px;
	padding-top: 4px;
	padding-bottom: 4px;
}

ul.bDot 
{
	padding-left: 0px; 
	margin: 0px; 
	list-style-type: none;
	line-height: 1.5;
}

ul.bDot li 
{
	padding-left: 15px;
	background: url(/images/bdot.png) 0px 0.5em no-repeat;
}

ul.rDot 
{
	padding-left: 0px; 
	margin: 0px; 
	list-style-type: none;
	line-height: 1.5;
}

ul.rDot li 
{
	padding-left: 15px;
	background: url(/images/rdot.png) 0px 0.5em no-repeat;
}

ul.gDot 
{
	padding-left: 0px; 
	margin: 0px; 
	list-style-type: none;
	line-height: 1.5;
}

ul.gDot li 
{
	padding-left: 15px;
	background: url(/images/gdot.png) 0px 0.5em no-repeat;
}

.Center
{
	margin:auto;
	position:absolute;
	top:0;
	left:0;
	bottom:0;
	right:0;
}

#divContents 
{
	max-width:849px;
	height:auto;
	min-height:550px;
	background-color:#FFF;
	margin:auto;
	padding:0px;
	border-width:7px 1px;
	border-style:solid;
	border-color:#A0C0FF;
	border-radius:0px;
}

#divHeader 
{
	height: 65px;
	background: #FFF;
	padding: 5px;
	margin-top: 5px;
}

#imgLogo 
{
	margin: 10px;
	height: 45px;
	float: left;
	border: 0px;
}

#divWelcome 
{
	margin-top: 30px;
	color: #46D;
	font-size: 12px;
	font-weight: 300;
	font-family: Roboto, Tahoma, Arial, Calibri, Sans-Serif;
	text-decoration: none;
}

#divMenu 
{
	margin-top: 2px;
	min-height: 20px;
	padding: 0px;
	border-top: 1px solid #DBE5FF;
	border-bottom: 2px solid #DBE5FF;
	background-color: #DBE5FF;
}

#tblMenu 
{
	align: center;
	width: 100%;
	border-collapse: collapse;
}

#divBanner {
	width: 100%;
	margin: auto;
	padding: 0px;
	height: 240px;
	background: url(/images/fb_banner.png) no-repeat 0px 0px;
}

#divBody 
{
	color: #5D5D5D;
	font-size: 12px;
	font-weight: 300;
	font-family: Roboto, Tahoma, Arial, Calibri, Sans-Serif;
}

#divFooter 
{
	margin: auto;
	padding: 5px;
	text-align: center;
	width: 100%;
}
	
#vSeparator 
{
	color: #46F;
	margin-left: 7px;
	margin-right: 3px;
	font-size: 11px;
	font-weight: 300;
	font-family: Roboto, Tahoma, Arial, Calibri, Sans-Serif;
}

#Mandatory 
{
	font-weight: bold;
	font-size: 14px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
	color: #FF9119;
}

.divMainBlock
{
	align:left;
	margin:auto;
}

.divInfoBlock
{
	display:inline-block;
	width:90%;
	max-width:375px;
	background:#F2F5FF;
	text-align:left;
	margin:7px;
	vertical-align:top;
	padding:7px;
	border-left:5px solid #DBE5F1;
	border-radius:0px 10px 10px 0px;
}

.divImpInfoBlock
{
	border-radius:0px;
	border-left:5px solid #DBE5F1;
	border-right:5px solid #DBE5F1;
}

.inputbox 
{
	color: #666;
	padding: 2px;
	font-size: 12px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
	border: 1px solid rgb(189,199,216);
	border-radius: 5px;
	overflow-y: auto;
	height: 18px;
}

.errorinputbox 
{
	color: #666;
	padding: 2px;
	font-size: 12px;
	height: 18px;
	font-weight: 300;
	font-family: Roboto, Tahoma, Arial;
	border: 1px solid rgb(255,0,0);
	border-radius: 5px;
	overflow-y: auto;
	height: 18px;
}

.mBtn 
{
	font-weight: bold;
	font-size: 14px;
	font-weight: 400;
	font-family: Roboto Condensed, Tahoma, Arial;
	color: #46F;
	text-align: center;
}

a.mBtn 
{
	font-weight: bold;
	font-size: 14px;
	font-weight: 400;
	font-family: Roboto Condensed, Tahoma, Arial;
	color: #46F;
	text-decoration: none;
	text-align: center;
}

a.mBtn:hover, a.mBtn:active 
{
	color: #F00;
	text-decoration: underline;
}

.normtext 
{
	color: #5D5D5D;
	font-size: 12px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
}

.normsmalltext 
{
	color: #5D5D5D;
	font-size: 11px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
}

.highlighted 
{
	color: #00F;
	font-size: 12px;
	font-weight: 300;
	font-family: Roboto, Tahoma, Arial;
}

.btn 
{
	color: #46D;
	font-size: 12px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
	text-decoration: none;
}

a.btn, a.btn:link, a.btn:visited 
{
	color: #46D;
	font-size: 12px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
	text-decoration: none;
}

a.btn:hover, a.btn:active 
{
	color: #F00;
	text-decoration: underline;
}

.title
{
	font-weight: bold;
	font-size: 11pt;
	font-weight: 400;
	font-family: Roboto Condensed, Tahoma, Arial;
	text-decoration: none;
	color: #46F;
}

.IdBtn
{
	padding: 2px 7px;
	line-height: 12px;
	color: #000;
	font-size: 11px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
	text-align: center;
	min-width: 65px;
}

._Tbl
{
	margin-top: 5px;
	padding: 2px;
	border: 1px solid #E0E0FF;
	background-color: white;
	border-collapse: collapse;
	border-radius: 5px;
}

._TblHead
{
	padding: 2px 6px 2px 6px;
	color: #222;
	font-size: 11px;
	font-weight: 500;
	font-family: Roboto, Tahoma, Arial;
	border-right: 1px solid #E0E0FF;
	background: url(/images/Tblhead.jpg) repeat-x 0px 0px;
	white-space: nowrap;
}

._TblCell
{
	padding: 2px 5px 2px 5px;
	color: #000;
	font-size: 11px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
	border-bottom: 1px solid #E0E0FF;
	border-right: 1px solid #E0E0FF;
	vertical-align: middle;
}

._pageLink
{
	text-align: center;
	border: 1px solid #DBE5FF;
	padding: 1px 4px 1px 4px;
}

#divPdata
{
	text-align: center;
	margin: 0px 10px 10px 0px;
	padding: 0px;
	border-width: 6px 1px;
	border-style: solid;
	border-color: #DBE5FF;
	width: 200px;
	height: auto;
	border-radius:7px;
	float:right;
}

#pData2
{
	padding: 5px;
	border: 1px solid #DBE5FF;
	border-width: 1px 0px;
	background-color: #FFF;
}

#lTD
{
	text-align: right;
	border-right: 1px solid #DBE5FF;
	padding: 2px 8px;
	vertical-align: middle;
}

#rTD
{
	text-align: left;
	padding: 2px 8px;
	vertical-align: middle;
}

#divPimage
{
	align: center;
	padding: 5px;
	border-radius: 5px;
	border: 1px solid #DBE5FF;
	width: 220px;
	height: 200px;
}

#pdtPrice 
{
	font-weight: bold;
	font-size: 22px;
	font-weight: 400;
	font-family: Roboto, Tahoma, Arial;
}

#divPdtInfo
{
	width: 98%;
	max-width: 250px;
	min-height: 250px; 
	height: auto;
	padding: 5px;
	margin: 5px;
	border: 1px solid #DBE5FF;
	border-bottom: 7px solid #DBE5FF;
}

.divHomePageBlock
{
	float:left;
	max-width:380px;
	display:inline-block;
	height: auto;
	margin: 5px;
	padding: 4px;
	border-radius: 5px;
	border: 1px solid #DBE5F1;
	background: #F2F5FF;
}

#debug
{
	align:center;
	width: 97%;
	border: 2px solid #CCC;
	margin: 5px;
	padding: 5px;
	height: 500px;
	overflow-y: scroll;
	background-color: #000;
	color: #0F0;
	font-size: 12px;
	font-weight: 300;
	font-family: Roboto, Tahoma, Arial;
}

@media only screen and (max-width : 500px)
{
}

#PrintOutput
{
	margin-bottom:15px;
	width:100%;
	line-height:1.5;
}

@media print {
	body * {
		visibility: hidden;
	}
	
	#PrintOutput * {
		visibility: visible;
	}
	
	#PrintOutput {
		position: absolute;
		left: 100px;
		top: 50px;
	}
}